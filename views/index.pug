extends layout

block content
  h1= title
  h2 Eksportér til komma separerede værdier (CSV)
    each typeName, type in types
      h3= typeName
      ul
        li
          a(href='/' + type + 's/csv') Alle
        each tag in tags
          li
            a(href='/' + type + 's/csv/' + tag)= 'Tagget med "' + tag + '"'
